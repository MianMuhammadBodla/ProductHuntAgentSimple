        "import os\n",
        "from typing import List, Dict, Any\n",
        "from langchain_core.prompts import PromptTemplate\n",
        "from langchain_core.output_parsers import StrOutputParser\n",
        "from langchain_core.runnables import RunnablePassthrough\n",
        "from langchain_google_genai import ChatGoogleGenerativeAI\n",
        "from exa_py import Exa\n",
        "import json\n",
        "\n",
        "class ProductHunterChatbot:\n",
        "    def __init__(self, google_api_key: str, exa_api_key: str):\n",
        "        \"\"\"\n",
        "        Initialize the Product Hunter Chatbot with updated Langchain integration\n",
        "\n",
        "        Args:\n",
        "            google_api_key (str): Google Gemini API Key\n",
        "            exa_api_key (str): Exa Search API Key\n",
        "        \"\"\"\n",
        "        # Set API Keys\n",
        "        os.environ[\"GOOGLE_API_KEY\"] = \""\n",
        "        os.environ[\"EXA_API_KEY\"] = \""\n",
        "\n",
        "\n",
        "        # Initialize LLM with updated method\n",
        "        self.llm = ChatGoogleGenerativeAI(model=\"gemini-pro\")\n",
        "\n",
        "        # Initialize Exa Search\n",
        "        self.exa_client = Exa(api_key=\"")\n",
        "\n",
        "        # Define Prompt Template for Product Hunting\n",
        "        self.product_prompt = PromptTemplate(\n",
        "            input_variables=[\"product_category\"],\n",
        "            template=\"\"\"You are a professional e-commerce product hunter.\n",
        "            Based on the following search results, help find the best products for the specified category.\n",
        "\n",
        "            Product Category: {product_category}\n",
        "\n",
        "             Search Results:\n",
        "            {search_results}\n",
        "\n",
        "     Provide detailed insights including:\n",
        "            1. Top recommended products\n",
        "            2. Price range\n",
        "            3. Competitive analysis\n",
        "            4. Quality assessment\n",
        "            5. Market trends\n",
        "\n",
        "            You should only search from the following domains:\n",
        "\n",
        "            [\"amazon.com\", \"ebay.com\", \"bestbuy.com\", \"walmart.com\", \"daraz.com\"]\n",
        "\n",
        "            Don't use the following domains:\n",
        "\n",
        "            [\n",
        "            \"wordpress.com\",\n",
        "            \"blogspot.com\",\n",
        "            \"medium.com\",\n",
        "            \"tumblr.com\",\n",
        "            \"wix.com\",\n",
        "            \"squarespace.com\",\n",
        "            \"ghost.io\",\n",
        "            \"weebly.com\",\n",
        "            \"jimdo.com\",\n",
        "            \"strikingly.com\",\n",
        "            \"livejournal.com\",\n",
        "            \"typepad.com\",\n",
        "            \"hubpages.com\",\n",
        "            \"blogger.com\",\n",
        "            \"over-blog.com\"\n",
        "        ]\n",
        "\n",
        "            Format your response in a clear and organized manner.\n",
        "            \"\"\"\n",
        "        )\n",
        "\n",
        "        # Create Runnable Chain\n",
        "        self.product_chain = (\n",
        "            {\"product_category\": RunnablePassthrough(), \"search_results\": RunnablePassthrough()}\n",
        "            | self.product_prompt\n",
        "            | self.llm\n",
        "            | StrOutputParser()\n",
        "        )\n",
        "\n",
        "                # Define list of known blog domains to exclude\n",
        "        self.blog_domains = [\n",
        "            \"wordpress.com\",\n",
        "            \"blogspot.com\",\n",
        "            \"medium.com\",\n",
        "            \"tumblr.com\",\n",
        "            \"wix.com\",\n",
        "            \"squarespace.com\",\n",
        "            \"ghost.io\",\n",
        "            \"weebly.com\",\n",
        "            \"jimdo.com\",\n",
        "            \"strikingly.com\",\n",
        "            \"livejournal.com\",\n",
        "            \"typepad.com\",\n",
        "            \"hubpages.com\",\n",
        "            \"blogger.com\",\n",
        "            \"over-blog.com\"\n",
        "        ]\n",
        "\n",
        "    def search_products(self, product_category: str, num_results: int = 5) -> List[Dict[str, Any]]:\n",
        "        \"\"\"\n",
        "        Search for products using Exa AI search\n",
        "\n",
        "        Args:\n",
        "            product_category (str): Category of products to search\n",
        "            num_results (int): Number of search results to retrieve\n",
        "\n",
        "        Returns:\n",
        "            List of product search results\n",
        "        \"\"\"\n",
        "        search_query = f\"Best {product_category} for e-commerce in 2024\"\n",
        "\n",
        "        try:\n",
        "            search_results = self.exa_client.search(\n",
        "                search_query,\n",
        "                num_results=num_results,\n",
        "                use_autoprompt=True,\n",
        "                type=\"keyword\",\n",
        "                # includeDomains=[\"amazon.com\", \"ebay.com\", \"bestbuy.com\", \"walmart.com\"],\n",
        "                # excludeDomains=self.blog_domains\n",
        "            )\n",
        "\n",
        "            # Process and extract relevant product information\n",
        "            processed_results = []\n",
        "            for result in search_results.results:\n",
        "                processed_results.append({\n",
        "                    \"title\": result.title,\n",
        "                    \"url\": result.url,\n",
        "                    \"score\": result.score\n",
        "                })\n",
        "\n",
        "            return processed_results\n",
        "\n",
        "        except Exception as e:\n",
        "            print(f\"Error in product search: {e}\")\n",
        "            return []\n",
        "\n",
        "    def interact(self, user_input: str) -> str:\n",
        "        \"\"\"\n",
        "        Main interaction method for the chatbot\n",
        "\n",
        "        Args:\n",
        "            user_input (str): User's message to the chatbot\n",
        "\n",
        "        Returns:\n",
        "            Chatbot's response\n",
        "        \"\"\"\n",
        "        try:\n",
        "            # Extract product category from user input\n",
        "            product_category = self.extract_product_category(user_input)\n",
        "\n",
        "            if not product_category:\n",
        "                return \"I'm sorry, I couldn't identify the product category from your input. Please try again.\"\n",
        "\n",
        "            # Search for products\n",
        "            search_results = self.search_products(product_category)\n",
        "\n",
        "            if not search_results:\n",
        "                return f\"No products found for category '{product_category}'. Please try a different category.\"\n",
        "\n",
        "            # Format search results as a string for the prompt\n",
        "            formatted_search_results = \"\\n\".join([\n",
        "                f\"Title: {item['title']}\\nURL: {item['url']}\\nScore: {item['score']}\\n\" for item in search_results\n",
        "            ])\n",
        "\n",
        "            # Analyze market using the LLM chain\n",
        "            market_analysis = self.product_chain.invoke({\n",
        "                \"product_category\": product_category,\n",
        "                \"search_results\": formatted_search_results\n",
        "            })\n",
        "\n",
        "            # Generate comprehensive response\n",
        "            response = f\"\"\"\n",
        "                          **Product Hunt Results for \"{product_category}\":**\n",
        "\n",
        "                          **Search Results:**\n",
        "                          {formatted_search_results}\n",
        "\n",
